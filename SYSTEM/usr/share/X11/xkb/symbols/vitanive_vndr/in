    key <AB01> {	[	  U0932,	NoSymbol,	U0926,		NoSymbol 	]	}; // Z ಲ 0CB2  
    key <AB02> {	[	  U0935,	NoSymbol,	U0927,		NoSymbol	]	}; // X ವ 0CB5  ಧ  0CA7
    key <AB03> {	[	  U0936,	NoSymbol,	U091E,		NoSymbol	]	}; // C ಶ 0CB6 ಞ  0C9E
    key <AB04> {	[	  U0937,	NoSymbol,	U0919,		NoSymbol	]	}; // V ಷ 0CB7 
    key <AB05> {	[	  U0938,	NoSymbol,	U092F,		NoSymbol	]	}; // B ಸ 0CB8  
    key <AB06> {	[	  U0939,	NoSymbol,	U0930,		NoSymbol	]	}; // N ಹ 0CB9 
    key <AB07> {	[	  U0933,	NoSymbol,	NoSymbol,	NoSymbol	]	}; // M ಳ 0CB3 ರ  0CB0
    key <AB08> {	[     comma,	less		]	};
    key <AB09> {	[    period,	greater		]	};
    key <AB10> {	[     slash,	question	]	};

    key <BKSL> {	[ backslash,         bar	]	};
    
    include "level3(ralt_switch)"
        
};



